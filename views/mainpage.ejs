                            <li class="nav-item ">
                                <a class="nav-link" href="#Shoes">Shoes</a>
                            </li>
                            <li class="nav-item ">
                                <a class="nav-link" href="#Phones">Phones</a>
                            </li>
                            <li class="nav-item ">
                                <a class="nav-link" href="#Bags">Bags</a>
                            </li>
                            <li class="nav-item ">
                                <a class="nav-link" href="#Watches">Watches</a>
                            </li>
                            <li class="nav-item ">
                                <a class="nav-link" href="#Clothes">Clothes</a>
                            </li>

                            <li class="nav-item">
                                <a class="nav-link" href="/cart"><i class="fa fa-shopping-cart" style="color: #ffff;" aria-hidden="true"></i></a>
                            </li>
                            <li class="nav-item name">
                                <i class="fa fa-user" aria-hidden="true"></i>
